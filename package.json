{
  "name": "data-morph",
  "version": "1.0.0",
  "description": "Data morphing utilities",
  "homepage": "https://github.com/muhsalaa/data-morph",
  "bugs": {
    "url": "https://github.com/muhsalaa/data-morph/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run"
  },
  "keywords": [
    "data",
    "morph",
    "transform",
    "structure"
  ],
  "author": "Muhammad Salma Abdul Aziz (https://emots.space)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhsalaa/data-morph.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
